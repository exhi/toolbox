{
    "devtools_port": 9090,
    "startup_app": {
        "name": "Dashboard",
        "description": "Dashboard",
        "url": "http://localhost:3000/dashboard/launcher",
        "icon": "http://localhost:3000/public/trayicon.png",
        "uuid": "xh-dashboard",
        "autoShow": true,
        "frame": false,
        "showTaskbarIcon": true
    },
    "runtime": {
        "arguments": "--enable-aggressive-domstorage-flushing",
        "version": "stable"
    },
    "services": [
        {
            "name": "layouts",
            "manifestUrl": "https://cdn.openfin.co/services/openfin/layouts/1.0.6/app.json",
            "config": {
                "tabstrip": {
                    "url": "http://localhost:3000/openfin-tabstrip",
                    "height": 42
                }
            }
        },
        {
            "name": "notifications"
        }
    ],
    "shortcut": {
        "company": "Extremely Heavy Industries",
        "description": "Dashboard",
        "name": "Dashboard"
    },
    "splashScreenImage": "http://localhost:3000/public/xh.png"
}