{
    "devtools_port": 9090,
    "startup_app": {
        "name": "OpenFin Example",
        "description": "OpenFin Example",
        "url": "http://localhost:3000/openfin",
        "applicationIcon": "http://localhost:3000/favicon.ico",
        "uuid": "xh-openfin-example",
        "autoShow": true,
        "frame": false
    },
    "runtime": {
        "arguments": "--enable-aggressive-domstorage-flushing",
        "version": "stable"
    },
    "services": [
        {
            "name": "layouts",
            "manifestUrl": "https://cdn.openfin.co/services/openfin/layouts/1.0.5/app.json",
            "config": {
                "tabstrip": {
                    "url": "http://localhost:3000/openfin-tabstrip",
                    "height": 42
                }
            }
        }
    ],
    "shortcut": {
        "company": "Extremely Heavy Industries",
        "description": "OpenFin Example Application",
        "name": "OpenFin Example"
    },
    "splashScreenImage": "http://localhost:3000/public/xh.png"
}